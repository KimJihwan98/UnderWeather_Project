<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.ssafit.model.dao.VideoDao">

    <select id="selectInterestViewFitViedo" resultType="Video">
    	select * from video order by viewCnt DESC
    </select>
    
    <select id="selectPartFitViedo" parameterType="string" resultType="Video">
    	select * from video where fitPartName = #{fitPartName}
    </select>
</mapper>